local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()
local Window = Rayfield:CreateWindow({
   Name = "Danny’s Hub Fisch",
   Icon = 0,
   LoadingTitle = "Danny’s Hub Fisch",
   LoadingSubtitle = "Script by HanzoRTX & Colton GUI by Colton",
   ShowText = "Rayfield", 
   Theme = "DarkBlue",




   ToggleUIKeybind = "K",




   DisableRayfieldPrompts = false,
   DisableBuildWarnings = false,
   ConfigurationSaving = {
      Enabled = true,
      FolderName = nil,
      FileName = "Big Hub"
   },




   Discord = {
      Enabled = false,
      Invite = "https://discord.gg/xDdhnjQzBC",
      RememberJoins = false 
   },




   KeySystem = true,
   KeySettings = {
      Title = "Danny's Hub Fisch",
      Subtitle = "Key System",
      Note = "Join discord for key https://discord.gg/xDdhnjQzBC",
      FileName = "DANNY-FISCH",
      SaveKey = false,
      GrabKeyFromSite = false,
      Key = {"DannyFisch1234"}
   }
})
local Tab = Window:CreateTab("MainFarm", 4483362458)
local Input = Tab:CreateInput({
   Name = "Discord : https://discord.gg/xDdhnjQzBC",
   CurrentValue = "",
   PlaceholderText = "",
   RemoveTextAfterFocusLost = false,
   Flag = "DiscordInvite",
   Callback = function(Text)
  -- script goes here
   end,
})
local Toggle = Tab:CreateToggle({
   Name = "Walk On Water",
   Flag = "WalkWater",
   Callback = function(Value)


   end,
})


local Toggle = Tab:CreateToggle({
   Name = "Auto Cast (not available rn)",
   CurrentValue = false,
   Flag = "Cast rod",
   Callback = function(Value)


   end,
})
local Toggle = Tab:CreateButton({
   Name = "Auto Catch (rejoin to turn off)",
   CurrentValue = false,
   Flag = "Catch Fish",
   Callback = function(Value)    
    local ReplicatedStorage = game:GetService("ReplicatedStorage")




local reelfinished = ReplicatedStorage.events.reelfinished




while true do
    reelfinished:FireServer(
        100,
        false
    )
    wait(.2)
end




    end,
})
local Toggle = Tab:CreateToggle({
   Name = "Auto Shake(rejoin to turn off)",
   CurrentValue = false,
   Flag = "RodShake",
   Callback = function(Value)
local vim = game:GetService("VirtualInputManager")
local playergui = game.Players.LocalPlayer.PlayerGui


while task.wait(0.05) do
    local shakeUi = playergui:FindFirstChild("shakeui")
    if shakeUi then
        local button = shakeUi:FindFirstChild("safezone") and shakeUi.safezone:FindFirstChild("button")
        if button and button.Visible then
            local pos = button.AbsolutePosition + button.AbsoluteSize / 2
            vim:SendMouseButtonEvent(pos.x, pos.y, 0, true, game, 1)  -- Press
            vim:SendMouseButtonEvent(pos.x, pos.y, 0, false, game, 1) -- Release
        end
    end
end   


   end,
})
local Tab = Window:CreateTab("Extra", 4483362458)
local Button = Tab:CreateButton({
   Name = "Infinite Yield",
   Callback = function()
loadstring(game:HttpGet("https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source"))()
   end,
})
