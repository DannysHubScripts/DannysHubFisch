      FileName = "Big Hub"
   },


   Discord = {
      Enabled = false,
      Invite = "https://discord.gg/xDdhnjQzBC",
      RememberJoins = false 
   },


   KeySystem = true,
   KeySettings = {
      Title = "Danny's Hub Fisch",
      Subtitle = "Key System",
      Note = "Join discord for key https://discord.gg/xDdhnjQzBC",
      FileName = "DANNY-FISCH",
      SaveKey = false,
      GrabKeyFromSite = false,
      Key = {"DANNY67"}
   }
})
local Tab = Window:CreateTab("MainFarm", 4483362458)
local Toggle = Tab:CreateToggle({
   Name = "Auto Cast",
   CurrentValue = false,
   Flag = "Cast rod (not available rn)",
   Callback = function(Value)
	-- script goes here
   end,
})
local Toggle = Tab:CreateButton({
   Name = "Auto Catch (rejoin to turn off)",
   CurrentValue = false,
   Flag = "Catch Fish",
   Callback = function(Value)    
    local ReplicatedStorage = game:GetService("ReplicatedStorage")


local reelfinished = ReplicatedStorage.events.reelfinished


while true do
    reelfinished:FireServer(
        100,
        false
    )
    wait(1)
end


    end,
})
